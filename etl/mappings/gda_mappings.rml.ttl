@prefix : <https://sakuna.ph/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#> .
@prefix ql: <http://www.w3.org/2004/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rml: <http://w3id.org/rml/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#MainLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda.csv";
    rml:referenceFormulation ql:CSV .

<#InciLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_incidents.csv" ;
    rml:referenceFormulation ql:CSV .

<#PrepLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_prep.csv" ;
    rml:referenceFormulation ql:CSV .

<#EvacLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_evac.csv" ;
    rml:referenceFormulation ql:CSV .

<#RescueLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_rescue.csv" ;
    rml:referenceFormulation ql:CSV .

<#CalamLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_calamity.csv" ;
    rml:referenceFormulation ql:CSV .

<#AffPopLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_aff_pop.csv" ;
    rml:referenceFormulation ql:CSV .

<#CasualtiesLogicalSource>
    a rml:LogicalSource;
    rml:source "data/gda_casualties.csv" ;
    rml:referenceFormulation ql:CSV .


<#EventMapping>
    a rr:TriplesMap;

    rml:logicalSource <#MainLogicalSource>;
    
    rr:subjectMap [
        rr:template "https://sakuna.ph/{id}";
    ];

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
            rml:functionExecution <#EventExecution> ;
            rml:termType rml:IRI;
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :eventName;
        rr:objectMap [
            rml:reference "eventName" ;
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasType ;
        rr:objectMap [
            rml:functionExecution <#TypeExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :startDate ;
        rr:objectMap [
            rml:reference "startDate" ;
            rml:datatype xsd:dateTime ;
        ];
    ] ;

    rr:predicateObjectMap [
        rr:predicate :endDate ;
        rr:objectMap [
            rml:reference "endDate" ;
            rml:datatype xsd:dateTime ;
        ];
    ] ;

    # rr:predicateObjectMap [
    #     rr:predicate :latitude ;
    #     rr:objectMap [
    #         rml:reference "latitude" ;
    #     ];
    # ] ;

    # rr:predicateObjectMap [
    #     rr:predicate :longitude ;
    #     rr:objectMap [
    #         rml:reference "longitude" ;
    #     ];
    # ] ;

    rr:predicateObjectMap [
        rr:predicate :hasLocation ;
        rr:objectMap [
            rml:functionExecution <#LocationExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasSubtype ;
        rr:objectMap [
            rml:functionExecution <#SubtypeExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasRelatedIncident ;
        rr:objectMap [
            rr:parentTriplesMap <#IncidentsMapping> ;
            rr:joinCondition [
                rr:child "id" ;
                rr:parent "id" ;
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness    ;
        rr:objectMap [

            rr:parentTriplesMap <#PreparednessMapping> ;
            rr:joinCondition [
                rr:child "id" ;
                rr:parent "id" ;
            ] ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness ;
        rr:objectMap [

            rr:parentTriplesMap <#EvacuationMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness ;
        rr:objectMap [

            rr:parentTriplesMap <#RescueMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#CalamMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#AffPopMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#CasualtiesMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    .

<#EventExecution>
    rml:function :toMajorIncident ;
    rml:input [
        rml:parameter grel:valueParameter;
        rml:inputValueMap [
            rml:reference "eventClass" ;
        ];
    ].

<#TypeExecution>
    rml:function :toTypeIRI;
    rml:input [
        rml:parameter grel:valueParameter;
        rml:inputValueMap [
            rml:reference "hasType";
        ];
    ].

<#SubtypeExecution>
    rml:function :toTypeIRI;
    rml:input [
        rml:parameter grel:valueParameter;
        rml:inputValueMap [
            rml:reference "hasSubtype";
        ];
    ].

<#LocationExecution>
    rml:function :matchLocationsToIRI;
    rml:input [
        rml:parameter grel:valueParameter;
        rml:inputValueMap [
            rml:reference "hasLocation";
            
        ];
    ].


<#IncidentsMapping>
    a rr:TriplesMap ;
    rml:logicalSource <#InciLogicalSource> ;

    rr:subjectMap [
        rr:template "https://sakuna.ph/{id}/related_incident/{sub_id}";
        rr:class :Incident ;
    ];

    rr:predicateObjectMap [
        rr:predicate :hasLocation ;
        rr:objectMap [
            rml:functionExecution <#LocationExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasType ;
        rr:objectMap [
            rml:functionExecution <#TypeExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    .

<#PreparednessMapping>
    a rr:TriplesMap ;
    rml:logicalSource <#PrepLogicalSource> ;

    rr:subjectMap [
        rr:template "https://sakuna.ph/{id}/preparedness";
        rr:class :Preparedness ;
    ];

    rr:predicateObjectMap [
        rr:predicate :agencyLGUsPresent ;
        rr:objectMap [
            rml:reference "agencyLGUsPresentPreparedness"
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :announcementsReleased ;
        rr:objectMap [
            rml:reference "announcementsReleased"
        ] ;
    ] ;

    .

<#EvacuationMapping>
