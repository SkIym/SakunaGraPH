<#EventMapping>
    a rr:TriplesMap;

    rml:logicalSource <#MainLogicalSource>;
    
    rr:subjectMap [
        rr:template "https://sakuna.ph/{id}";
    ];

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
            rml:functionExecution <#EventExecution> ;
            rml:termType rml:IRI;
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :eventName;
        rr:objectMap [
            rml:reference "eventName" ;
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasType ;
        rr:objectMap [
            rml:functionExecution <#TypeExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :startDate ;
        rr:objectMap [
            rml:reference "startDate" ;
            rml:datatype xsd:dateTime ;
        ];
    ] ;

    rr:predicateObjectMap [
        rr:predicate :endDate ;
        rr:objectMap [
            rml:reference "endDate" ;
            rml:datatype xsd:dateTime ;
        ];
    ] ;

    # rr:predicateObjectMap [
    #     rr:predicate :latitude ;
    #     rr:objectMap [
    #         rml:reference "latitude" ;
    #     ];
    # ] ;

    # rr:predicateObjectMap [
    #     rr:predicate :longitude ;
    #     rr:objectMap [
    #         rml:reference "longitude" ;
    #     ];
    # ] ;

    rr:predicateObjectMap [
        rr:predicate :hasLocation ;
        rr:objectMap [
            rml:functionExecution <#LocationExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasSubtype ;
        rr:objectMap [
            rml:functionExecution <#SubtypeExecution>;
            rml:termType rml:IRI;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasRelatedIncident ;
        rr:objectMap [
            rr:parentTriplesMap <#IncidentsMapping> ;
            rr:joinCondition [
                rr:child "id" ;
                rr:parent "id" ;
            ];
        ];
    ];

    # Preparedness

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness    ;
        rr:objectMap [

            rr:parentTriplesMap <#PreparednessMapping> ;
            rr:joinCondition [
                rr:child "id" ;
                rr:parent "id" ;
            ] ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness ;
        rr:objectMap [

            rr:parentTriplesMap <#EvacuationMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasPreparedness ;
        rr:objectMap [

            rr:parentTriplesMap <#RescueMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#CalamMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    # Impact

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#AffPopMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#CasualtiesMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#HousingMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#InfraMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#GenDmgMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#PowerMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#CommsMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#RNBMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#SeaportMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasImpact ;
        rr:objectMap [

            rr:parentTriplesMap <#WaterMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    # Response

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#AssistanceMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefGMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefWMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefCMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefMMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefUMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate :hasResponse ;
        rr:objectMap [

            rr:parentTriplesMap <#ReliefGenMapping> ;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];

    .

<#EventExecution>
    rml:function :toMajorIncident ;
    rml:input [
        rml:parameter grel:valueParameter;
        rml:inputValueMap [
            rml:reference "eventClass" ;
